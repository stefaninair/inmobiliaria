@model Inmobiliaria.Models.TipoInmueble

@{
    ViewData["Title"] = "Eliminar Tipo de Inmueble";
}

<h1>Eliminar Tipo de Inmueble</h1>

<h3>¿Estás seguro que quieres eliminar este tipo de inmueble?</h3>
<hr />

<div class="row">
    <div class="col-md-6">
        <dl class="row">
            <dt class="col-sm-4">
                @Html.DisplayNameFor(model => model.Id)
            </dt>
            <dd class="col-sm-8">
                @Html.DisplayFor(model => model.Id)
            </dd>

            <dt class="col-sm-4">
                @Html.DisplayNameFor(model => model.Nombre)
            </dt>
            <dd class="col-sm-8">
                @Html.DisplayFor(model => model.Nombre)
            </dd>
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Eliminar" class="btn btn-danger" />
            <a class="btn btn-secondary" asp-action="Index">Volver a la lista</a>
        </form>
    </div>
</div>
